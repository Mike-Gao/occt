# for "sfixpcu" command
pload XSDRAW

restore [locate_data_file cts20170_base.rle] base
restore [locate_data_file cts20170_face.rle] cont

sfixpcu base base -f
sfixpcu cont cont -f

checkshape base
checkshape cont

explode base face

featprism base cont cont 0 0 1 1 1
featperformval prism result 25

set square 47886.5
