puts "============"
puts "shading"
puts "============"
puts ""
#######################################################
puts "Tests display modes changing in the IVtk view"
#######################################################

set anImage $imagedir/${casename}.png

pload MODELING
ivtkinit
psphere s 30
ivtkdisplay s
ivtksetdispmode 1
ivtkproperty s -shading gouraud
ivtkdump $anImage
