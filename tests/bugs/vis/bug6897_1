puts "============"
puts "0006897: Impossible to change edge line type in 3D View"
puts "============"
puts ""

pload MODELING VISUALIZATION
vclear
vinit View1
vaxo

box b 1 1 1
vdisplay -dispMode 1 b
vfit

vaspects b -setInterior SOLID
vdump $imagedir/${casename}_3.png

vaspects b -setInterior SOLID_WIREFRAME -setEdgeType DASH -setEdgeColor 0.392 1.0 0.392
vdump $imagedir/${casename}_1.png

vaspects b -setInterior HOLLOW
vdump $imagedir/${casename}_2.png
