puts ""

# for "sfixpcu" command
pload XSDRAW

restore [locate_data_file CTO900_cts16184a.rle] a 
sfixpcu a a -f
checkshape a

restore [locate_data_file cts16184b.brep] b
sfixpcu b b -f
checkshape b

bop a b
bopfuse result a b
checkshape -top result

set square 0.0892544
set 2dviewer 0


